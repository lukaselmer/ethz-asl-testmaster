<%= simple_form_for(@test_run_log) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :logged_at %>
    <%= f.input :message_type %>
    <%= f.input :message_content %>
    <%= f.input :execution_in_microseconds %>
    <%= f.association :test_run %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
